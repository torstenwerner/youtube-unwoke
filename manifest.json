{
    "manifest_version": 3,
    "name": "Hide YouTube Clarify Box",
    "version": "1.0",
    "description": "Hides the clarify box on YouTube",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }